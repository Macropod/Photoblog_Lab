<div class="container">
	<h1>Update an existing gallery</h1>

	<div class="row">
	  <div class="span6 offset3">
	    <%= form_for(@gallery) do |f| %>
	    	<%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
			<%= render 'fields', f: f %>
	    <% end %>
	  </div>
	</div>
</div>